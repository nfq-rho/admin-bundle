{% trans_default_domain 'adminInterface' %}
{% import '@NfqAdmin/_macro/generic.html.twig' as macro %}
<!-- Sidebar user panel -->
<div class="user-panel">
    <div class="pull-left image">
        {{ macro.avatar(user.avatar, user.username)  }}
    </div>
    <div class="pull-left info">
        <p>{{ user.email }}</p>

        <a href="#"><i class="fas fa-circle text-success"></i> {{ 'admin.generic.online'|trans }}</a>
    </div>
</div>
